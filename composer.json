{
    "name": "surpaimb/laravel-wechat",
    "description": "微信 SDK for Laravel",
    "keywords": ["wechat", "weixin","laravel", "sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "surpaimb",
            "email": "surpaimb@126.com"
        }
    ],
    "require": {
        "illuminate/container": "^5.1 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "surpaimb/wechat": "^5.0"
    },
    "require-dev": {
        "laravel/framework": "^8.5",
        "friendsofphp/php-cs-fixer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Surpaimb\\LaravelWeChat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Surpaimb\\LaravelWeChat\\ServiceProvider"
            ],
            "aliases": {
                "Surpaimb\\WeChat": "Surpaimb\\LaravelWeChat\\Facade"
            }
        }
    }
}
